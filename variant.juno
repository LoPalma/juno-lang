import Io;

opt int test() {
    Io.println("test");
}

void main() {
	// variant types demo
	Io.println("This program demonstrates variant types");

    opt int y = test();

	string|int x = "";
	x = 5;
	Io.println("X's integer value is: " ^^ x);
	x = ":D";
	Io.println("X's string value is '" ^^ x ^^ "'");
}
